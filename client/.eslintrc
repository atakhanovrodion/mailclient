{
  "extends": ["airbnb-typescript", "eslint-config-prettier"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "project": "./tsconfig.json",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "strict": ["error", "never"]
  },
  "settings": {
    "import/core-modules": ["electron"]
  }
}
