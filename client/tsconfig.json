{
  "compilerOptions": {
    "module": "CommonJS",
    "noEmit": true,
    "target": "ES5",
    "noImplicitAny": true,
    "lib": ["es2015", "ES2016", "es2017", "ES6", "dom"],
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": "./src",
    "noImplicitThis": true,
    "strictNullChecks": true,
    "allowJs": true
  }
}
